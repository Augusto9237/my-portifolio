datasource db {
  provider = "postgresql"
  url      = env("PORTFOLIO_POSTGRES_PRISMA_URL")
}

generator client {
  provider = "prisma-client-js"
}


model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Messages {
  id        Int     @id @default(autoincrement())
  name      String
  email     String  
  phone     String
  message   String
  createdAt DateTime @default(now())
}
